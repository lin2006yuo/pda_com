@charset "utf-8";
html,body {
    margin:0;
    padding:0;
    border:0;
    font-size:14px;
}
/* 调整分辨率,兼容不同分辨率的页面*/
@media screen and (min-width: 320px){
    html,body {
        font-size:16px;
    }
}
@media screen and (min-width: 400px){
    html,body {
        font-size:20px;
    }
    .mint-header{
        height: 2.5em;
    }
}
body {
    /*background:#f5f5f3;*/
    font-family:'Microsoft YaHei';
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    /* Safari */
}
.mhome {
    padding:0;
    min-width:320px;
    max-width:640px;
    margin:0 auto;
}
.viewport {
    padding:0 20px;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}
div,p,ul,li,ol,dl,dt,dd,span,img,input,table,h1,h2,h3,h4,h5,h6 {
    margin:0;
    padding:0;
    border:0;
    list-style:none;
    list-style-type:none;
}
i,em {
    font-style:normal;
}
.fl {
    float:left;
}
.fr {
    float:right;
}
.ce {
    margin:0 auto;
    text-align:center;
}
.cl {
    clear:both;
}
a {
    text-decoration:none;
}
a:visited {
    text-decoration:none;
}
/*=====================================================*/
.k1 {
    text-align: center;
    width:100%;
}
.k2 {
    width:80%;
}

input[type="submit"],input[type="reset"],input[type="button"] {
    -webkit-appearance:none;
}

a,button,input,textarea {
    -webkit-tap-highlight-color:rgba(0,0,0,0);
}

input::-webkit-outer-spin-button,input::-webkit-inner-spin-button {
    -webkit-appearance:none !important;
}
@-moz-document url-prefix() {
    select {
        width: 6em;
        height: 2em;
        font-size: inherit;
        border: 1px solid #EFF2F7;
        border-radius: 4px;
        padding-left: 5px;
        appearance:none;
        -moz-appearance:none;
        -webkit-appearance:none;
    }
    option {
        font-weight: normal;
        display: block;
        white-space: pre;
        font-size: 1em;
        min-height: 1.2em;
        padding: 0px 2px 1px;
        color:#333;
    }
}
select::-ms-expand { display: none; }
select{
    width: 6em;
    height: 2em;
    font-size: inherit;
    border: 1px solid #EFF2F7;
    border-radius: 4px;
    padding-left: 5px;
    appearance:none;
    -moz-appearance:none;
    -webkit-appearance:none;
}
option {
    font-weight: normal;
    display: block;
    white-space: pre;
    font-size: 1em;
    min-height: 1.2em;
    padding: 0px 2px 1px;
    color:#333;
}
button{
    height: 2em;
    font-size: inherit;
    border: 1px solid #EFF2F7;
    border-radius: 4px;
    color: #24a8e6;
    background-color: #fff;
}
